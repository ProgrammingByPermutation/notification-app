<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
      stylesheets="@app.css" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="org.nullinside.notification_app.controllers.AppController">
    <HBox VBox.vgrow="NEVER">
        <Button mnemonicParsing="false" onAction="#addTwitchChat" text="Add Twitch Chat"/>
        <Button mnemonicParsing="false" onAction="#showSettings" text="Settings"/>
    </HBox>
    <VBox fx:id="alarmList" VBox.vgrow="ALWAYS" styleClass="outlined_alerts"/>
</VBox>
